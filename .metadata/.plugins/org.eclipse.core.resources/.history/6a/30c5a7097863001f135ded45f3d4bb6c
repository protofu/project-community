package com.kosta.domain;

import lombok.Data;

@Data
public class UserDTO {
	private Long id;
	private String name, nickname, email, password;
	private UserRole role;

	@Builder
	public UserDTO(Long id, String name, String nickname, String email, String password, UserRole role) {
		this.id = id;
		this.name = name;
		this.nickname = nickname;
		this.email = email;
		this.password = password;
		this.role = role;
	}
	
	
}
